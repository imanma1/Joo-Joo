<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="50.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.apps.messages.view.list.GroupInMessagesListController">
   <children>
      <Label fx:id="groupNameLabel" layoutX="21.0" layoutY="16.0" prefHeight="18.0" text="Group name">
         <font>
            <Font name="Constantia" size="14.0" />
         </font>
      </Label>
      <Button fx:id="showMessagesBtn" layoutX="389.0" layoutY="14.0" mnemonicParsing="false" onAction="#showMessages" text="Show Messages">
         <font>
            <Font name="Constantia" size="12.0" />
         </font>
      </Button>
      <Label fx:id="unseenMessagesLabel" layoutX="320.0" layoutY="6.0" prefHeight="38.0" prefWidth="53.0" text="(unseen &#10;messages)&#10;" wrapText="true">
         <font>
            <Font name="Constantia" size="12.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
