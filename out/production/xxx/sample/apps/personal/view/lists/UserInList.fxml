   <children>
      <Label fx:id="usernameLabel" layoutX="21.0" layoutY="16.0" prefHeight="18.0" text="Username">
         <font>
            <Font name="Constantia" size="14.0" />
         </font>
      </Label>
      <Button fx:id="showProfileBtn" layoutX="403.0" layoutY="14.0" mnemonicParsing="false" onAction="#showProfile" text="Show Profile">
         <font>
            <Font name="Constantia" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
